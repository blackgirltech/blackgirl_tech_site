<header>
    <i class="fa fa-bars menu-btn" aria-hidden="true"></i>
    <div id="headerInner">
        <nav>
            <div id="navWrap">
                <span id="left">
                    <%= link_to "About", about_path%>
                    <%= link_to "Events", events_path %>
                    <%= link_to "Donate", new_donation_path %>
                </span>
                <%= image_tag "white_logo" %>
                <span id="right">
                    <%= link_to "Opportunites", opportunities_path %>
                    <% if member_signed_in? %>
                      <%# if admin? %>
                        <%#= link_to 'Admin', admin_root_path %>
                      <%# end %>
                      <%= link_to "My Profile", member_path(current_member) %>
                      <%= link_to "Log Out", destroy_member_session_path, method: :delete %>
                    <% else %>
                      <%= link_to "Join Us", new_member_registration_path %>
                      <%= link_to "Log In", new_member_session_path %>
                    <% end %>
                </span>
            </div>
        </nav>
    </div>
</header>
